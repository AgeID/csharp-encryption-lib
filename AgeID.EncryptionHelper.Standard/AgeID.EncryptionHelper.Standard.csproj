<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>AgeID</RootNamespace>
    <AssemblyName>AgeID.EncryptionHelper.NETStandard</AssemblyName>
    <Authors>AgeID</Authors>
    <Description>Class Library for .NET Standard that allows encrypting and decrypting AgeID payloads</Description>
    <PackageIconUrl>https://www.ageid.com/images/site/logo-head.png</PackageIconUrl>
    <PackageTags>AgeID</PackageTags>
    <PackageReleaseNotes>V2 release</PackageReleaseNotes>
    <Version>2.0.0</Version>
    <PackageId>AgeID.EncryptionHelper.NETStandard</PackageId>
    <Product>AgeID.EncryptionHelper.NETStandard</Product>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <OutputPath>bin\Release\</OutputPath>
    <DocumentationFile></DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
  </ItemGroup>

</Project>
